<h1>New User</h1>


<%= form_tag(users_url, method: 'post') do %>


  <div>
    <%= label_tag :email %><br />
    <%= text_field_tag :email %>
  </div>

  <div>
    <%= label_tag :password_digest %><br />
    <%= text_field_tag :password_digest %>
  </div>

  <div>
    <%= label_tag :gender %><br />
    <%= text_field_tag :gender %>
  </div>

  <div>
    <%= label_tag :age %><br />
    <%= text_field_tag :age %>
  </div>


  <div>
    <%= submit_tag "Create User" %>
  </div>
<% end %>
